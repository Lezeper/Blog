<!DOCTYPE html>
<html ng-app="myApp">
<head><% include ./partials/head.ejs %></head>
<body ng-controller="myController as myCtrl" ng-cloak>
<div class="container">
    <% include ./partials/nav.ejs %>
    <!--------------------------- Contents ------------------------------>
    <div class="panel panel-default col-md-4 col-md-offset-4">
        <div class="panel-body">
            <form novalidate>
                <h2 class="text-center">Login</h2>
                <fieldset class="form-group">
                    <label for="username">Username</label>
                    <input type="text" id="username" class="form-control" ng-model="username">
                </fieldset>
                <fieldset class="form-group">
                    <label for="password">Password</label>
                    <input type="password" id="password" class="form-control" ng-model="password">
                </fieldset>
                <button class="btn btn-primary">Login</button>
            </form>
        </div>
    </div>
</div>

<% include ./partials/jsFile.ejs %>
</body>
<% include ./partials/footer.ejs %>
</html>
